{
    "preguntas": [
        {
            "id": "hanok_address_input",
            "type": "text",
            "step": 1
        },
        

        {
            "id": "hanok_comprar_interes_zona",
            "type": "select",
            "values": [ "solo_inmueble", "interes_zona" ],
            "step": 2
        },
        {
            "id": "hanok_comprar",
            "type": "select",
            "values": [ "primera_vivienda" , "segunda_residencia" , "cambio_vivienda" , "inversion" ],
            "step": 2,
            "hidden" : true
        },
        {
            "id": "hanok_hipoteca",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_tasacion_oficial",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_2_viv_vender",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 2,
            "hidden": true
        },


        {
            "id": "hanok_superficie",
            "type": "number",
            "step": 3
        },
        {
            "id": "hanok_dormitorios",
            "type": "select",
            "values": [ "1", "2", "3", "4+" ],
            "step": 3
        },
        {
            "id": "hanok_wc",
            "type": "select",
            "values": [ "1", "2", "3+" ],
            "step": 3
        },


        {
            "id": "hanok_tipo_vivienda",
            "type": "select",
            "values": [ "piso" , "chalet_adosado" , "casa_independiente" ],
            "step": 4
        },
        {
            "id": "hanok_antiguedad_viv",
            "type": "select",
            "values": [ "nueva" , "segunda_mano" ],
            "step": 4
        },
        {
            "id": "hanok_estado_inmueble",
            "type": "select",
            "values": [ "excelente", "aceptable", "necesita_reforma" ],
            "step": 4
        },


        {
            "id": "hanok_viv_extras",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 5
        },
        {
            "id": "hanok_trastero",
            "type": "checkbox",
            "step": 5
        },

        {
            "id": "hanok_garaje",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_terraza",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_ascensor",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_piscina",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_cuando_comprar",
            "type": "select",
            "values": [ "3_6_meses", "+6_meses" ],
            "step": 5
        },
        {
            "id": "hanok_interes_ahorro_suministros",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 5
        },
        

        
        
        {
            "id": "hanok_nombre",
            "type": "text",
            "step": 6
        }
        ,
        {
            "id": "hanok_email",
            "type": "email",
            "step": 6
        },
        {
            "id": "hanok_telefono",
            "type": "tel",
            "step": 6
        }
        ,
        {
            "id": "aceptacion-politica",
            "type": "checkbox",
            "step": 6
        }
    ],



    "reglas": [
        {
            "target": "hanok_hipoteca",
            "showWhen": {
                "or": [
                    { "eq": [ "hanok_comprar" , "primera_vivienda" ]},
                    {"eq": [ "hanok_comprar" , "segunda_residencia" ]},
                    {"eq": [ "hanok_comprar" , "inversion" ]}
                ]
            }
        },
        {
            "target": "hanok_tasacion_oficial",
            "showWhen": {
                "eq": [ "hanok_hipoteca" , "si" ]
            }
        },
        {
            "target": "hanok_2_viv_vender",
            "showWhen": {
                "eq": [ "hanok_comprar" , "cambio_vivienda" ]
            }
        },
        {
            "target": "hanok_extras",
            "showWhen": {
                "eq": [ "hanok_viv_extras", "si" ]
            }
        }
    ]
}
