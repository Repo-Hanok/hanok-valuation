{
    "preguntas": [
        {
            "id": "hanok_address_input",
            "type": "text",
            "step": 1
        },

        {
            "id": "hanok_cuando_vender",
            "type": "select",
            "values": [ "ya" , "cuanto_antes" , "3_a_6_meses" , "mas_6_meses" ],
            "step": 2
        },
        {
            "id": "hanok_donde_casa_anunciada",
            "type": "select",
            "values": [ "anunciada_en_plataformas" , "anunciada_en_rrss" , "anunciada_en_inmobiliaria" ],
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_venta_exclusiva",
            "type": "select",
            "values": [ "si" , "no" ],
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_exclusiva_cuantos_meses",
            "type": "number",
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_motivo_vender",
            "type": "select",
            "values": [ "cambio_casa", "necesidad_economica", "herencia", "cambio_ciudad", "otro" ],
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_motivo_3m",
            "type": "select",
            "values": [ "reforma" , "reforma" , "asuntos_personales", "otro" ],
            "step": 2,
            "hidden": true
        },
        {
            "id": "hanok_mot_6m",
            "type": "select",
            "values": [ "no_prisa" , "situacion_legal", "segunda_vivienda", "otro" ],
            "step": 2,
            "hidden": true
        },
        
        
        {
            "id": "hanok_precio_esperado",
            "type": "number",
            "step": 3
        },
        {
            "id": "hanok_intento_venta_anterior",
            "type": "select",
            "values": [ "si_solo", "si_agencia", "no" ],
            "step": 3
        },
        {
            "id": "hanok_tipo_vivienda",
            "type": "select",
            "values": [ "piso" , "chalet_adosado" , "casa_independiente" ],
            "step": 3
        },
        {
            "id": "hanok_relacion_inmueble",
            "type": "select",
            "values": [ "propietario", "inquilino", "comprador", "agente" ],
            "step": 3
        },
        {
            "id": "hanok_estado_inmueble",
            "type": "select",
            "values": [ "excelente", "aceptable", "necesita_reforma" ],
            "step": 3
        },


        {
            "id": "hanok_superficie",
            "type": "number",
            "step": 4
        },
        {
            "id": "hanok_dormitorios",
            "type": "select",
            "values": [ "1", "2", "3", "4+" ],
            "step": 4
        },
        {
            "id": "hanok_wc",
            "type": "select",
            "values": [ "1", "2", "3+" ],
            "step": 4
        },




        {
            "id": "hanok_viv_exterior",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 5
        },
        {
            "id": "hanok_viv_extras",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 5
        },
        {
            "id": "hanok_trastero",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_garaje",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_terraza",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_ascensor",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_piscina",
            "type": "checkbox",
            "step": 5
        },
        {
            "id": "hanok_viv_en_uso",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 5
        },



        {
            "id": "hanok_libre_de_hipoteca",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 6
        },
        {
            "id": "hanok_viv_okupada",
            "type": "select",
            "values": [ "si", "no" ],
            "step": 6
        },
        {
            "id": "hanok_proceso_venta",
            "type": "select",
            "values": [ "vender_rapido", "mejor_precio", "proceso_sencillo" ],
            "step": 6
        },
        {
            "id": "hanok_canal_contacto",
            "type": "select",
            "values": [ "llamada", "whatsapp", "email" ],
            "step": 6
        },




        {
            "id": "hanok_nombre",
            "type": "text",
            "step": 7
        }
        ,
        {
            "id": "hanok_email",
            "type": "email",
            "step": 7
        },
        {
            "id": "hanok_telefono",
            "type": "tel",
            "step": 7
        }
        ,
        {
            "id": "aceptacion-politica",
            "type": "checkbox",
            "step": 7
        }
    ],











    "reglas": [
        {
            "target": "hanok_donde_casa_anunciada",
            "showWhen": {
                "eq": [ "hanok_cuando_vender", "ya" ]
            }
        },
        {
            "target": "hanok_venta_exclusiva",
            "showWhen": {
                "eq": [ "hanok_donde_casa_anunciada" , "anunciada_en_inmobiliaria" ]
            }
        },
        {
            "target": "hanok_exclusiva_cuantos_meses",
            "showWhen": {
                "eq": [ "hanok_venta_exclusiva" , "si" ]
            }
        },
        {
            "target": "hanok_motivo_vender",
            "showWhen": {
                "eq": [ "hanok_cuando_vender", "cuanto_antes" ]
            }
        },
        {
            "target": "hanok_motivo_3m",
            "showWhen": {
                "eq": [ "hanok_cuando_vender", "3_a_6_meses" ]
            }
        },
        {
            "target": "hanok_mot_6m",
            "showWhen": {
                "eq": [ "hanok_cuando_vender", "mas_6_meses" ]
            }
        },
        {
            "target": "hanok_extras",
            "showWhen": {
                "eq": [ "hanok_viv_extras", "si" ]
            }
        }
        
    ]
}
